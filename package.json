    "elasticsearch": "^12.0.1",
    "font-awesome": "^4.7.0",
    "jimp": "^0.2.28",
    "jquery": "^3.1.0",
    "jquery.facedetection": "^2.0.2",
    "musicmetadata": "^2.0.5",
    "request": "^2.75.0",
    "smartcrop": "^1.1.1",
    "tar.gz": "^1.0.5",
    "tree-kill": "^1.1.0",
    "yauzl": "^2.8.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "devtron": "^1.4.0",
    "electron": "^1.7.1",
    "electron-builder": "^19.4.0"
  },
  "build": {
    "appId": "com.theyolk.app",
    "compression": "maximum",
    "asar": true,
    "copyright": "Copyright Â© year ${author}",
    "linux": {
      "target": [
        "rpm"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "pkg"
      ]
    }
  },
  "scripts": {
    "pack": "build --dir",
    "build": "build -l --ia32 --x64",
    "mac": "build -m",
    "Yolk-dev": "cross-env ELECTRON_ENV='development' ./node_modules/.bin/electron ./",
    "Yolk": "./node_modules/.bin/electron ./"
